{
	"name": "Some Yellow",
	"type": "dark",
	"colors": {
		//editor
		"editor.background": "#272625",
		//"editor.foreground": "#f9d423",
		//------
		//file tabs
		"tab.activeForeground": "#f9d423",
		//
		//-----activity bar
		"activityBar.background": "#272625",
		"activityBar.foreground": "#f9d423",
		"activityBarBadge.background": "#f9d423",
		//-----------
		//-------side bar
		"sideBarSectionHeader.background": "#272625",
		"sideBarSectionHeader.foreground": "#f9d423",
		"sideBarSectionHeader.border": "#f9d423",
		"sideBarTitle.foreground": "#f9d423",
		"sideBar.background": "#272625",
		//---------
		//status bar
		"statusBar.background": "#272625",
		"statusBar.foreground": "#f9d423",

		"statusBar.noFolderBackground": "#272625",
		"statusBar.noFolderForeground": "#f9d423",
		
		"statusBar.debuggingBackground": "#f9d423",
		"statusBar.debuggingForeground": "#272625",
		//-------------
		//notification
		"notifications.background": "#272625",
		"notifications.foreground": "#f9d423",
		"notifications.border": "#f9d423",
		"notificationCenterHeader.background": "#272625",
		"notificationCenterHeader.foreground": "#f9d423",

		"notificationLink.foreground": "#f9d423",
		
		"list.hoverBackground": "#272625",
		"list.hoverForeground": "#f9d423",


		//tab
		"titleBar.activeBackground": "#272625",
		"titleBar.activeForeground": "#f9d423",
		"titleBar.inactiveBackground": "#272625",
		"titleBar.inactiveForeground": "#f9d423",
		
		"button.background": "#383837",
		"button.foreground": "#f9d423",

		"input.border": "#f9d423",
		"textLink.foreground": "#f9d423",

		"progressBar.background": "#f9d423",
		"checkbox.background": "#272625",
		"checkbox.foreground": "#f9d423",
		"checkbox.border" : "#f9d423"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#707070"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#f9d423",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ff4e50"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#fe4365"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#f9d423"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#e53a86"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f9d423"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ffff"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
